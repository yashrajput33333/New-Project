{
    "version": 2,
    "builds": [
      {
        "src": "Backend/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Backend/src/index.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@MONGODB_URI",
      "PORT": "3000",
      "CORS_ORIGIN": "@CORS_ORIGIN"
    }
  }
  